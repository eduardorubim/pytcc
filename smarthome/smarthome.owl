<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xml:base="http://www.semanticweb.org/rmyamguchi/ontologies/2019/7/smarthome.owl"
         xmlns="http://www.semanticweb.org/rmyamguchi/ontologies/2019/7/smarthome.owl#">

<owl:Ontology rdf:about="http://www.semanticweb.org/rmyamguchi/ontologies/2019/7/smarthome.owl">
  <rdfs:comment>A smarthome ontology</rdfs:comment>
</owl:Ontology>

<owl:ObjectProperty rdf:about="#hasSmartDevice">
  <rdfs:domain rdf:resource="#Place"/>
  <rdfs:range rdf:resource="#SmartDevice"/>
  <owl:inverseOf rdf:resource="#isSmartDeviceOf"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isSmartDeviceOf"/>

<owl:DatatypeProperty rdf:about="#isNamed">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:domain rdf:resource="#Place"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#isOn">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:domain rdf:resource="#SmartDevice"/>
</owl:DatatypeProperty>

<owl:Class rdf:about="#Place">
  <owl:disjointWith rdf:resource="#SmartDevice"/>
</owl:Class>

<owl:Class rdf:about="#SmartDevice"/>

<owl:Class rdf:about="#IndoorPlace">
  <rdfs:subClassOf rdf:resource="#Place"/>
  <owl:disjointWith rdf:resource="#OutdoorPlace"/>
</owl:Class>

<owl:Class rdf:about="#OutdoorPlace">
  <rdfs:subClassOf rdf:resource="#Place"/>
</owl:Class>

<IndoorPlace rdf:about="#Bathroom">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
